{
  "mcpServers": {
    "neozork": {
      "command": "python3",
      "args": [
        "neozork_mcp_server.py"
      ],
      "env": {
        "PYTHONPATH": "${workspaceFolder}",
        "LOG_LEVEL": "INFO",
        "USE_UV": "true",
        "UV_ONLY": "true"
      },
      "cwd": "${workspaceFolder}"
    },
    "neozork-docker": {
      "command": "docker",
      "args": [
        "compose",
        "run",
        "--rm",
        "-T",
        "-e",
        "PYTHONPATH=/app",
        "-e",
        "LOG_LEVEL=INFO",
        "-e",
        "DOCKER_CONTAINER=true",
        "-e",
        "USE_UV=true",
        "-e",
        "UV_ONLY=true",
        "neozork-hld",
        "python3",
        "neozork_mcp_server.py"
      ],
      "env": {
        "DOCKER_HOST": "unix:///var/run/docker.sock"
      },
      "cwd": "${PROJECT_ROOT}"
    }
  },
  "serverSettings": {
    "neozork": {
      "enabled": true,
      "autoStart": true,
      "debug": false,
      "logLevel": "info",
      "features": {
        "financial_data": true,
        "technical_indicators": true,
        "github_copilot": true,
        "code_completion": true,
        "project_analysis": true,
        "ai_suggestions": true,
        "uv_integration": true,
        "uv_only_mode": true
      },
      "performance": {
        "max_files": 15000,
        "max_file_size": "10MB",
        "cache_enabled": true,
        "cache_size": "200MB",
        "indexing_timeout": 60,
        "completion_timeout": 5,
        "memory_limit_mb": 512
      },
      "monitoring": {
        "enable_monitoring": true,
        "health_check_interval": 60,
        "max_memory_mb": 512,
        "max_cpu_percent": 80,
        "log_rotation": {
          "max_size_mb": 100,
          "backup_count": 5
        },
        "alerts": {
          "memory_threshold": 80,
          "cpu_threshold": 90,
          "restart_threshold": 3
        }
      },
      "security": {
        "allowed_paths": [
          "${PROJECT_ROOT}"
        ],
        "max_file_size": 10485760,
        "timeout": 30000,
        "validate_inputs": true,
        "sanitize_paths": true
      },
      "docker": {
        "enabled": true,
        "container_name": "neozork-mcp-server",
        "port": 8080,
        "volumes": [
          "${PROJECT_ROOT}:/workspace",
          "${PROJECT_ROOT}/logs:/workspace/logs"
        ],
        "environment": {
          "PYTHONPATH": "/workspace/src:/workspace",
          "LOG_LEVEL": "INFO",
          "USE_UV": "true",
          "UV_ONLY": "true"
        }
      },
      "development": {
        "auto_reload": false,
        "hot_reload": false,
        "debug_mode": false,
        "profiling": false,
        "coverage": false
      }
    },
    "neozork-docker": {
      "enabled": false,
      "autoStart": false,
      "debug": false,
      "logLevel": "info",
      "features": {
        "financial_data": true,
        "technical_indicators": true,
        "github_copilot": true,
        "code_completion": true,
        "project_analysis": true,
        "ai_suggestions": true,
        "docker_integration": true,
        "real_time_monitoring": true,
        "uv_integration": true,
        "uv_only_mode": true
      }
    }
  },
  "cursor": {
    "mcp": {
      "autoStart": true,
      "logLevel": "info",
      "features": {
        "completion": true,
        "hover": true,
        "definition": true,
        "references": true,
        "workspaceSymbols": true,
        "diagnostics": true,
        "codeActions": true,
        "formatting": true
      },
      "preferences": {
        "defaultServer": "neozork",
        "fallbackToDocker": true,
        "useUV": true,
        "uvOnly": true,
        "dockerEnabled": true
      }
    },
    "python": {
      "interpreter": "python3",
      "packageManager": "uv",
      "uvPath": "uv",
      "autoInstallDependencies": true,
      "uvOnly": true
    }
  },
  "financial_data": {
    "auto_scan": true,
    "scan_interval": 300,
    "supported_formats": [
      "csv",
      "parquet",
      "json"
    ],
    "data_directories": [
      "data",
      "mql5_feed",
      "financial_data"
    ],
    "symbol_patterns": [
      "*_*",
      "*USD*",
      "*EUR*",
      "*GBP*"
    ],
    "timeframe_patterns": [
      "*D1*",
      "*H1*",
      "*M15*",
      "*M5*",
      "*M1*"
    ]
  },
  "technical_indicators": {
    "trend": [
      "SMA",
      "EMA",
      "ADX",
      "SAR",
      "HMA"
    ],
    "oscillators": [
      "RSI",
      "Stochastic",
      "CCI"
    ],
    "momentum": [
      "MACD",
      "Stochastic_Oscillator"
    ],
    "volatility": [
      "ATR",
      "Bollinger_Bands",
      "Standard_Deviation"
    ],
    "volume": [
      "OBV",
      "VWAP"
    ],
    "support_resistance": [
      "Donchian_Channels",
      "Fibonacci_Retracements",
      "Pivot_Points"
    ],
    "predictive": [
      "Time_Series_Forecast"
    ],
    "probability": [
      "Kelly_Criterion",
      "Monte_Carlo"
    ],
    "sentiment": [
      "COT",
      "Fear_Greed",
      "Social_Sentiment"
    ]
  },
  "code_snippets": {
    "imports": [
      {
        "name": "import_pandas",
        "description": "Import pandas library",
        "code": "import pandas as pd"
      },
      {
        "name": "import_numpy",
        "description": "Import numpy library",
        "code": "import numpy as np"
      },
      {
        "name": "import_matplotlib",
        "description": "Import matplotlib for plotting",
        "code": "import matplotlib.pyplot as plt"
      }
    ],
    "data_loading": [
      {
        "name": "read_csv",
        "description": "Read CSV file",
        "code": "df = pd.read_csv('data.csv')"
      },
      {
        "name": "read_parquet",
        "description": "Read Parquet file",
        "code": "df = pd.read_parquet('data.parquet')"
      }
    ],
    "analysis": [
      {
        "name": "calculate_rsi",
        "description": "Calculate RSI indicator",
        "code": "rsi = calculate_rsi(df['close'], period=14)"
      },
      {
        "name": "calculate_macd",
        "description": "Calculate MACD indicator",
        "code": "macd = calculate_macd(df['close'])"
      },
      {
        "name": "backtest_strategy",
        "description": "Backtest trading strategy",
        "code": "results = backtest_strategy(data, strategy_params)"
      }
    ],
    "visualization": [
      {
        "name": "plot_data",
        "description": "Plot financial data",
        "code": "plt.plot(df['close'])\nplt.show()"
      }
    ],
    "docker": [
      {
        "name": "docker_setup",
        "description": "Start Docker services",
        "code": "docker compose up -d"
      },
      {
        "name": "docker_mcp",
        "description": "Run MCP server in Docker",
        "code": "docker compose run --rm neozork-hld python3 neozork_mcp_server.py"
      }
    ],
    "uv": [
      {
        "name": "uv_install",
        "description": "Install dependencies with uv",
        "code": "uv pip install -r requirements.txt"
      },
      {
        "name": "uv_sync",
        "description": "Sync dependencies with uv",
        "code": "uv sync"
      },
      {
        "name": "uv_run",
        "description": "Run script with uv",
        "code": "uv run python3 script.py"
      }
    ],
    "testing": [
      {
        "name": "run_tests",
        "description": "Run tests",
        "code": "uv run python -m pytest tests/ -v"
      }
    ],
    "kubernetes": [
      {
        "name": "kubectl_deploy",
        "description": "Deploy application to Kubernetes",
        "code": "kubectl apply -f k8s/neozork-apple-deployment.yaml"
      },
      {
        "name": "kubectl_status",
        "description": "Check deployment status",
        "code": "kubectl get pods -l app=neozork-interactive"
      },
      {
        "name": "kubectl_logs",
        "description": "View application logs",
        "code": "kubectl logs -f deployment/neozork-interactive-apple"
      },
      {
        "name": "kubectl_scale",
        "description": "Scale deployment",
        "code": "kubectl scale deployment neozork-interactive-apple --replicas=3"
      },
      {
        "name": "kubectl_port_forward",
        "description": "Port forward to access service locally",
        "code": "kubectl port-forward service/neozork-interactive-service 8080:80"
      },
      {
        "name": "kubectl_describe",
        "description": "Describe deployment details",
        "code": "kubectl describe deployment neozork-interactive-apple"
      },
      {
        "name": "kubectl_rollout",
        "description": "Rollout restart deployment",
        "code": "kubectl rollout restart deployment/neozork-interactive-apple"
      },
      {
        "name": "kubectl_delete",
        "description": "Delete deployment",
        "code": "kubectl delete -f k8s/neozork-apple-deployment.yaml"
      },
      {
        "name": "kubectl_exec",
        "description": "Execute command in pod",
        "code": "kubectl exec -it deployment/neozork-interactive-apple -- /bin/bash"
      },
      {
        "name": "kubectl_top",
        "description": "View resource usage",
        "code": "kubectl top pods -l app=neozork-interactive"
      }
    ]
  },
  "ai_suggestions": {
    "enabled": true,
    "context_aware": true,
    "financial_patterns": true,
    "code_quality": true,
    "performance_optimization": true,
    "best_practices": true
  },
  "kubernetes": {
    "enabled": true,
    "namespace": "neozork",
    "deployment": {
      "replicas": 2,
      "strategy": "RollingUpdate",
      "maxSurge": 1,
      "maxUnavailable": 0,
      "platforms": {
        "apple-silicon": {
          "nodeSelector": {
            "kubernetes.io/arch": "arm64"
          },
          "image": "neozork-interactive:apple-latest",
          "resources": {
            "requests": {
              "memory": "2Gi",
              "cpu": "1000m"
            },
            "limits": {
              "memory": "4Gi",
              "cpu": "2000m"
            }
          }
        },
        "x86": {
          "nodeSelector": {
            "kubernetes.io/arch": "amd64"
          },
          "image": "neozork-interactive:latest",
          "resources": {
            "requests": {
              "memory": "1Gi",
              "cpu": "500m"
            },
            "limits": {
              "memory": "2Gi",
              "cpu": "1000m"
            }
          }
        }
      }
    },
    "service": {
      "type": "LoadBalancer",
      "port": 80,
      "targetPort": 8080,
      "annotations": {
        "service.beta.kubernetes.io/aws-load-balancer-type": "nlb"
      }
    },
    "ingress": {
      "enabled": true,
      "host": "neozork.local",
      "tls": {
        "enabled": true,
        "secretName": "neozork-tls"
      },
      "annotations": {
        "nginx.ingress.kubernetes.io/rewrite-target": "/",
        "nginx.ingress.kubernetes.io/ssl-redirect": "true"
      }
    },
    "persistentVolumes": {
      "data": {
        "size": "10Gi",
        "accessMode": "ReadWriteOnce",
        "storageClass": "fast-ssd"
      },
      "logs": {
        "size": "5Gi",
        "accessMode": "ReadWriteOnce",
        "storageClass": "standard"
      },
      "plots": {
        "size": "5Gi",
        "accessMode": "ReadWriteOnce",
        "storageClass": "standard"
      },
      "results": {
        "size": "10Gi",
        "accessMode": "ReadWriteOnce",
        "storageClass": "fast-ssd"
      }
    },
    "monitoring": {
      "enabled": true,
      "prometheus": {
        "enabled": true,
        "scrapeInterval": "30s",
        "metricsPath": "/metrics"
      },
      "grafana": {
        "enabled": true,
        "dashboard": "neozork-dashboard"
      },
      "healthChecks": {
        "livenessProbe": {
          "httpGet": {
            "path": "/health",
            "port": 8080
          },
          "initialDelaySeconds": 30,
          "periodSeconds": 10,
          "timeoutSeconds": 5,
          "failureThreshold": 3
        },
        "readinessProbe": {
          "httpGet": {
            "path": "/ready",
            "port": 8080
          },
          "initialDelaySeconds": 5,
          "periodSeconds": 5,
          "timeoutSeconds": 3,
          "failureThreshold": 3
        }
      }
    },
    "security": {
      "serviceAccount": {
        "create": true,
        "name": "neozork-service-account"
      },
      "rbac": {
        "enabled": true,
        "rules": [
          {
            "apiGroups": [""],
            "resources": ["pods", "services", "endpoints"],
            "verbs": ["get", "list", "watch"]
          },
          {
            "apiGroups": ["apps"],
            "resources": ["deployments"],
            "verbs": ["get", "list", "watch"]
          }
        ]
      },
      "networkPolicy": {
        "enabled": true,
        "ingress": [
          {
            "from": [
              {
                "namespaceSelector": {
                  "matchLabels": {
                    "name": "ingress-nginx"
                  }
                }
              }
            ],
            "ports": [
              {
                "protocol": "TCP",
                "port": 8080
              }
            ]
          }
        ],
        "egress": [
          {
            "to": [],
            "ports": [
              {
                "protocol": "TCP",
                "port": 443
              },
              {
                "protocol": "TCP",
                "port": 80
              }
            ]
          }
        ]
      },
      "podSecurityPolicy": {
        "enabled": false
      }
    },
    "autoscaling": {
      "enabled": true,
      "minReplicas": 2,
      "maxReplicas": 10,
      "targetCPUUtilizationPercentage": 70,
      "targetMemoryUtilizationPercentage": 80,
      "behavior": {
        "scaleDown": {
          "stabilizationWindowSeconds": 300,
          "policies": [
            {
              "type": "Percent",
              "value": 10,
              "periodSeconds": 60
            }
          ]
        },
        "scaleUp": {
          "stabilizationWindowSeconds": 0,
          "policies": [
            {
              "type": "Percent",
              "value": 100,
              "periodSeconds": 15
            }
          ]
        }
      }
    },
    "configMaps": {
      "app-config": {
        "data": {
          "PYTHONPATH": "/app",
          "LOG_LEVEL": "INFO",
          "USE_UV": "true",
          "UV_ONLY": "true"
        }
      },
      "ml-config": {
        "data": {
          "ML_MODEL_PATH": "/app/models",
          "BATCH_SIZE": "32",
          "MAX_WORKERS": "4"
        }
      }
    },
    "secrets": {
      "api-keys": {
        "type": "Opaque",
        "data": {
          "FINANCIAL_API_KEY": "",
          "ML_API_KEY": ""
        }
      }
    }
  }
}