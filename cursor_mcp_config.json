{
  "mcpServers": {
    "neozork": {
      "command": "python",
      "args": ["neozork_mcp_server.py"],
      "env": {
        "PYTHONPATH": "/app"
      }
    }
  },
  "serverSettings": {
    "neozork": {
      "enabled": true,
      "autoStart": true,
      "debug": false,
      "logLevel": "info",
      "features": {
        "financial_data": true,
        "technical_indicators": true,
        "code_completion": true,
        "project_analysis": true,
        "ai_suggestions": true
      }
    }
  }
} 